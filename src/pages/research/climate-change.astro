---
import { getEntry, render } from 'astro:content'
import Passworder from '../../components/passworder.svelte'
import Layout from '../../layouts/Layout.astro'
import Research from '../../layouts/research.astro'
const article = await getEntry('research', 'the-ineffectiveness-of-climate-change-policies-in-the-us')
const RederedArticle = await render(article)
const RenderedArticleContent = RederedArticle.Content

---

<Layout title="The Ineffectiveness of Climate Change Policies in the United States" description="">
	<Passworder password={"please"} client:load >
		<Research content={article.data}>
			<RenderedArticleContent />
		</Research>
	</Passworder>
</Layout>